<html>
<head>

</head>
<body>
    <h1>Index Page</h1>
    <div id="products">

    </div>
    <p>Mobile</p>
    <button type="button" id="getData">Get Data</button>
</body>
</html>

<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
<script>

    $(document).ready(function () {
        $("#getData").click(function () {

            $.ajax(
                {
                url: 'http://localhost:63177/home/productlist',
                type: 'get',
                contentType: 'application/json',
                success: function (resData) {
                    $.each(resData, function (index) {
                        $("#products").append("<p>" + resData[index].name + "</p>");
                        //alert(resData[index].name);
                    })
                },
                error: function (xhr) {
                    alert("Somethign Wrong");
                }
            })
        })
    });

</script>